public class Book{
  public String title;
  public String author;
  public String price;
  public String weight;
  public double isbn; 
  
  public Book(String title, String author, String price, String weight, double isbn){
    this.title = title;
    this.author = author;
    this.price = price;
    this.weight = weight;
    this.isbn = isbn;
  }
}