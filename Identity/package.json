{
  "name": "Identity",
  "version": "1.0.0",
  "description": "App build of manual user auth for Datafiniti Product Developer position",
  "main": "index.js",
  "repository": "git@github.com:sdkindy/DataFizz.git",
  "scripts": {
    "prestart": "./prestart.sh",
    "start": "NODE_ENV=development npm-run-all --parallel open:db open:src open:server lint:watch",
    "open:src": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "open:db": "ttab -G mongod",
    "open:server": "ttab -d ./server npm run dev",
    "lint": "node_modules/.bin/esw webpack.config.* src",
    "lint:watch": "npm run lint -- --watch --ext .js --ext .jsx --color",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Sonam D. Kindy",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^2.2.1",
    "jsdom": "8.5.0",
    "mocha": "2.4.5",
    "react-addons-test-utils": "15.0.2",
    "webpack": "1.13.1",
    "webpack-dev-server": "^1.14.0",
    "autoprefixer": "^6.4.0",
    "babel-cli": "6.8.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-airbnb-base": "^3.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-watch": "^2.1.14",
    "eventsource-polyfill": "0.9.6",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "nock": "8.0.0",
    "node-sass": "^3.8.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "resolve-url-loader": "^1.6.0",
