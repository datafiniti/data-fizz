    "axios": "^0.15.1",
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^3.10.1",
    "material-ui": "^0.16.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.0.4",
    "redux-form": "^6.1.1",
